      expect         : "^[Yy]es - .*"
      dont_echo_cmd  : YES
    </custom_item>


    <custom_item>
      type           : CMD_EXEC
      description    : "CIS Control 3 (3.4(b)) Deploy Automated Operating System Patch Management Tools"
      info           : "Does the organization have any exceptions in place for any endpoint devices as it applies to patches being up to date?"
      see_also       : "https://controls-assessment-specification.readthedocs.io/en/latest"
