<check_type:"Unix">


    <custom_item>
      type           : CMD_EXEC
      description    : "CIS Control 1 (1.4) Maintain Detailed Asset Inventory"
      info           : "Implementation Group 1 - Does the organization have an established policy or procedure detailing how assets/devices are added or removed from inventory?"
      see_also       : "https://controls-assessment-specification.readthedocs.io/en/latest"
      reference      : "CSCv7|1.4"
# Note: Variable @1_4_Answer@ replaced with "No" in field "cmd".
# Note: Variable @1_4_Support@ replaced with "None" in field "cmd".
# Note: Variable @Attesting_User@ replaced with "Attesting User" in field "cmd".
      cmd            : "printf 'No - Attesting User\\nPolicy or Policy Statement: None'"
      expect         : "^[Yy]es - .*"
      dont_echo_cmd  : YES
    </custom_item>


    <custom_item>
      type           : CMD_EXEC
      description    : "CIS Control 1 (1.6) Ensure that unauthorized assets are removed, quarantined or the inventory is updated"
      info           : "Implementation Group 1 - Does the organization have an established policy or procedure specifying a timeframe for which assets/devices are removed from inventory or inventory is updated?"
      see_also       : "https://controls-assessment-specification.readthedocs.io/en/latest"
      reference      : "CSCv7|1.6"
# Note: Variable @1_6_Answer@ replaced with "No" in field "cmd".
# Note: Variable @1_6_Support@ replaced with "999" in field "cmd".
# Note: Variable @Attesting_User@ replaced with "Attesting User" in field "cmd".
      cmd            : "printf 'No - Attesting User\\nTimeframe for removing/updating assets (days): 999'"
      expect         : "^[Yy]es - .*"
      dont_echo_cmd  : YES
    </custom_item>


    <custom_item>
      type           : CMD_EXEC
      description    : "CIS Control 2 (2.1(b)) Maintain and Inventory of Authorized Software"
      info           : "Implementation Group 1 - Does the Organization have an established policy defining a timeframe to to remediate software issues, such as removing unauthorized applications and/or patching out of date/unsupported software?"
      see_also       : "https://controls-assessment-specification.readthedocs.io/en/latest"
      reference      : "CSCv7|2.1"
# Note: Variable @2_1_2_Answer@ replaced with "No" in field "cmd".
# Note: Variable @2_1_2_Support@ replaced with "None" in field "cmd".
# Note: Variable @Attesting_User@ replaced with "Attesting User" in field "cmd".
      cmd            : "printf 'No - Attesting User\\nLocation of Policy or Policy Statement: None'"
      expect         : "^[Yy]es - .*"
      dont_echo_cmd  : YES
    </custom_item>


    <custom_item>
      type           : CMD_EXEC
      description    : "CIS Control 2 (2.1(a)) Maintain and Inventory of Authorized Software"
      info           : "Implementation Group 1 - Does the Organization have a current list of approved software?"
      see_also       : "https://controls-assessment-specification.readthedocs.io/en/latest"
      reference      : "CSCv7|2.1"
# Note: Variable @2_1_1_Answer@ replaced with "No" in field "cmd".
# Note: Variable @2_1_1_Support@ replaced with "None" in field "cmd".
# Note: Variable @Attesting_User@ replaced with "Attesting User" in field "cmd".
